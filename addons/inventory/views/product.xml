<odoo>
  <data>
    <!-- explicit list view definition -->
     <record id="product_template_product_form" model="ir.ui.view">
            <field name="name">product_template_product_form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='uom_po_id']" position="after">
                   <field name="aire_sante"/>
                </xpath> 

                <xpath expr="//field[@name='uom_po_id']" position="after">
                   <field name="region"/>
                </xpath>

               <!-- <xpath expr="//field[@name='uom_po_id']" position="after">
                   <field name="num_PVR"/>
                </xpath> -->

                <xpath expr="//field[@name='product_tag_ids']" position="after">
                   <field name="nom_de_la_structure"/>
                </xpath>

                <xpath expr="//field[@name='uom_po_id']" position="after">
                   <field name="district_sante"/>
                </xpath>

                <xpath expr="//field[@name='product_tag_ids']" position="after">
                   <field name="atc_classe" string="Classe ATC"/>
                </xpath> 

                <xpath expr="//field[@name='product_tag_ids']" position="after">
                   <field name="code_cip"/>
                </xpath>

                <xpath expr="//field[@name='aire_sante']" position="after">
                   <field name="fabricant"/>
                </xpath> 

                <xpath expr="//field[@name='nom_de_la_structure']" position="after">
                   <field name="dci_id"/>
                </xpath>

                <xpath expr="//field[@name='dci_id']" position="after">
                   <field name="famille"/>
                </xpath>

                <xpath expr="//field[@name='nom_de_la_structure']" position="after">
                   <field name="tableau"/>
                </xpath>

                <xpath expr="//field[@name='famille']" position="after">
                   <field name="laboratoire_id"/>
                </xpath>

                <xpath expr="//field[@name='region']" position="after">
                   <field name="contingente"/>
                </xpath>

                <xpath expr="//field[@name='nom_de_la_structure']" position="after">
                   <field name="distributeur_id"/>
                </xpath>

                <xpath expr="//field[@name='fabricant']" position="after">
                   <field name="gamme_id"/>
                </xpath>

                <xpath expr="//field[@name='fabricant']" position="after">
                   <field name="couleur_id"/>
                </xpath>

                <xpath expr="//field[@name='laboratoire_id']" position="after">
                   <field name="famille_id"/>
                </xpath>

            </field>
      </record>

      <record id="view_laboratoire_tree" model="ir.ui.view">
        <field name="name">laboratoire.tree</field>
        <field name="model">laboratoire</field>
        <field name="arch" type="xml">
            <tree string="Laboratoire">
                <field name="code"/>
                <field name="name"/>
                <field name="contact_name"/>
                <field name="tel"/>
            </tree>
        </field>
    </record>
	
    <record id="view_laboratoire_form" model="ir.ui.view">
        <field name="name">laboratoire.form</field>
        <field name="model">laboratoire</field>
        <field name="arch" type="xml">
            <form string="Laboratoire">
               <group col="4">
                  <field name="code"/>
                  <field name="name"/>
                  <field name="contact_name"/>
                  <field name="tel"/>
               </group>
            </form>
        </field>
    </record>
	
	<record id="view_laboratoire_filter" model="ir.ui.view">
		<field name="name">laboratoire.list.select</field>
		<field name="model">laboratoire</field>
		<field name="priority" eval="15"/>
		<field name="arch" type="xml">
			<search string="Search Laboratoire">
				<field name="code"/>
            <field name="name"/>
				<field name="contact_name"/>
				<field name="tel"/>
		   </search>
		</field>
	</record>
	
	<record id="action_laboratoire" model="ir.actions.act_window">
		<field name="name">Laboratoire</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">laboratoire</field>
		<field name="view_mode">tree,form</field>
		<field name="search_view_id" ref="view_laboratoire_filter"/>
		<field name="context">{}</field>
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Nouveau Laboratoire 
			</p>
		</field>
	</record>

    <menuitem id="inventory.laboratoire_menu"  parent="stock.menu_product_in_config_stock" action="action_laboratoire"
     name="Laboratoire"/>

	
	<record id="view_gamme_tree" model="ir.ui.view">
        <field name="name">gamme.tree</field>
