        <field name="arch" type="xml">
            <form string="Gamme">
               <group col="4">
                  <field name="code"/>
                  <field name="name"/>
               </group>
            </form>
        </field>
    </record>
	
	<record id="view_gamme_filter" model="ir.ui.view">
		<field name="name">gamme.list.select</field>
		<field name="model">gamme</field>
		<field name="priority" eval="15"/>
		<field name="arch" type="xml">
			<search string="Search Gamme">
				<field name="code"/>
            <field name="name"/>
		   </search>
		</field>
	</record>
	
	<record id="action_gamme" model="ir.actions.act_window">
		<field name="name">Gamme</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">gamme</field>
		<field name="view_mode">tree,form</field>
		<field name="search_view_id" ref="view_gamme_filter"/>
		<field name="context">{}</field>
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Nouvelle Gamme
			</p>
		</field>
	</record>

    <menuitem id="inventory.gamme_menu"  parent="stock.menu_product_in_config_stock" action="action_gamme"
     name = "Gamme"/> 



      <record id="product_template_tree" model="ir.ui.view">
            <field name="name">product.template.tree</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_tree_view"/>
            <field name="arch" type="xml">
              <tree string="Product" multi_edit="1" sample="1">
                <header>
                  <button string="Print Labels" type="object" name="action_open_label_layout"/>
                </header>
                <field name="atc_classe" optional="hide"/>
              </tree>
            </field>
      </record> 

      <record id="product_template_search" model="ir.ui.view">
        <field name="name">product.template.searches</field>
        <field name="model">product.template</field>
        <field name="arch" type="xml">
            <search string="Product"> 
               <field name="atc_classe" filter_domain="[('atc_classe', 'child_of', raw_value)]"/>
                <separator/>
                <group expand="1" string="Group By">
                    <filter string="Classe ATC" name="atc_classe" context="{'group_by':'atc_classe'}"/>
                </group>
            </search>
         </field>
     </record>

     <record id="product_template_action" model="ir.actions.act_window">
        <field name="name">Product</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">kanban,tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new products
            </p>
        </field>
    </record>

