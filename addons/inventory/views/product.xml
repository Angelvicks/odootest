		   </search>
		</field>
	</record>
	
	<record id="action_gamme" model="ir.actions.act_window">
		<field name="name">Gamme</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">gamme</field>
		<field name="view_mode">tree,form</field>
		<field name="search_view_id" ref="view_gamme_filter"/>
		<field name="context">{}</field>
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				New Gamme
			</p>
		</field>
	</record>

    <menuitem id="inventory.gamme_menu"  parent="stock.menu_product_in_config_stock" action="action_gamme"
     name = "Gamme"/> 



      <record id="product_template_tree" model="ir.ui.view">
            <field name="name">product.template.tree</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="attributes">
                    <attribute name="string">Product</attribute>
                    <attribute name="multi_edit">1</attribute>
                    <attribute name="sample">1</attribute>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <header>
                        <button string="Print Labels" type="object" name="action_open_label_layout"/>
                    </header>
                    <field name="atc_classe" optional="hide"/>
                    <field name="lot_number" optional="hide"/>
                </xpath>
            </field>
       </record>

       <record id="view_product_template_form" model="ir.ui.view">
            <field name="name">product.template.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/> <!-- Replace with the correct external ID -->
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[position()=4]" position="after">
                    <!-- Add a new page to the notebook -->
                    <page string="Lots/Serial Numbers">
                        <!-- Your content for the Lot Infos page goes here -->
                        <field name="lot_number"/>
                        <field name="lot_prices"/>

                        
                            <tree string="Product Templates">
                                <field name="name"/>
                              <!--  <field name="lot_ids" widget="one2many_list"/> -->
                            </tree>
                        

                    </page>
                </xpath>
            </field>
       </record>


      <record id="product_template_search" model="ir.ui.view">
        <field name="name">product.template.searches</field>
        <field name="model">product.template</field>
        <field name="arch" type="xml">
            <search string="Product"> 
               <field name="atc_classe" filter_domain="[('atc_classe', 'child_of', raw_value)]"/>
                <separator/>
                <group expand="1" string="Group By">
                    <filter string="ATC Class" name="atc_classe" context="{'group_by':'atc_classe'}"/>
                </group>
            </search>
         </field>
     </record>

     <record id="product_template_action" model="ir.actions.act_window">
        <field name="name">Product</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">kanban,tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new product
            </p>
        </field>
    </record>

