 <odoo>
 
 <template id="report_pvdereception">
      <t t-call="web.html_container">
         <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <div class="oe_structure"/>
                           <div> 
                           <h1 style="text-align:center; font-weight: bold;">Product Acceptance Report</h1><br/>
                            <div class="row p-4 text-center">

                                <div class="col-3">
                                    <p><span style="font-weight: bold;">Region : </span>
                                       <span > Littoral </span>
                                    <!--  <span t-field="doc.region"/> -->
                                    </p>

                                    <p> <span style="font-weight: bold;"> Health District : </span>
                                        <span> Akwa </span>
                                   <!-- <span t-field="doc.district_sante"></span> -->
                                    </p>
                                </div>

                                <div class="col-3">
                                  <p>
                                    <span style="font-weight: bold;">Health Area : </span>
                                    <span > Douala </span>
                                  <!--  <span t-field="doc.aire_sante"/> -->

                                  </p>
                                  
                                  <p>
                                    <span style="font-weight: bold;">Name of the Structure : </span>
                                    <span t-field="doc.company_id" > </span> 
                                   <!-- <span t-field="doc.nom_de_la_structure" /> -->
                                  </p>


                                </div>

                                <div class="col-6">
                                    <p class="text-center"> <span style="font-weight: bold;">Supplier : </span>
                                    <span t-field="doc.partner_ids"></span> </p>

                                    <div class="row">
                                        <div class="col-6">
                                           <p> <span style="font-weight: bold;"> Delivery Dates :</span>
                                            <span t-field="doc.stock_dates"> </span> Days </p>
                                            
                                            <p> <span style="font-weight: bold;"> N° BL : </span> 
                                              <!--  <span> P0005 </span> -->
                                              <span t-field="doc.stock_names"/> 
                                            </p>
                                              

                                        </div>
                                        <div class="col-6">
                                            <span style="font-weight: bold;">N° PVR: 
                                            <span style="border-bottom: 1px solid black; height: 20px; width: 100px;"> </span>
                                             <!-- <span t-field="doc.num_PVR"/> -->
                                            </span>

                                            <p> <span style="font-weight: bold;">N°BC : </span>
                                            <span t-field="doc.stock_origin"/>
                                            <!--  <span t-field="doc.num_bc"/> -->
                                            <!--  <t t-if="doc.purchase_order_name">
                                                <span t-field="doc.purchase_order_name"/> 
                                              </t>   -->
                                            </p>
                                              
                                        </div>

                                        
                                    </div>
                               
                                    
                                </div>
                                
                            </div>
                              <table class="table" style="width: 100%;" >
                                <thead>
                                   <tr>
                                        <th name="th_numero" class="text-start">
                                            <span>Order no.</span>
                                        </th>
                                        <th name="th_nom" class="text-start">
                                            <span>Name (DCI) </span>
                                        </th>
                                        <th name="th_dosage" class="text-start">
                                            <span>Dosage</span>
                                        </th>
                                        <th name="th_forme" class="text-start">
                                            <span>Galenic Form</span>
                                        </th>
                                        <th name="th_conditionement" class="text-start">
                                            <span>Packaging</span>
                                        </th>
                                        <th name="th_quantité" class="text-start">
                                            <span>Quantity (Delivery Slip)</span>
                                        </th>
                                        <th name="th_qtitéRe" class="text-start">
                                            <span>Quantity Received</span>
                                        </th>
                                        <th name="th_numérodelot" class="text-start">
                                            <span>Lot Numbers</span>
                                        </th>
                                        <th name="th_date" class="text-start">
                                            <span>Expiration Date</span>
                                        </th>
                                        <th name="th_fabricant" class="text-start">
                                            <span>Manufacturer</span>
                                        </th>
                                         <th name="th_écart" class="text-start">
                                            <span>Difference</span>
                                        </th>
                                        <th name="th_qtitéRej" class="text-start">
                                            <span>Rejected Quantity</span>
                                        </th>
                                         <th name="th_qtitéAcc" class="text-start">
                                            <span>Accepted Quantity</span>
                                        </th>
                                         <th name="th_observations" class="text-start">
                                            <span>Observations</span>
                                        </th>
                            
                                    </tr>
                                </thead>

                              <!--  <tbody> -->
                                 <t t-foreach="docs" t-as="doc"> 
                                  <tr>
                                        <td style="width: 10%;" >
                                            <span t-field="doc.id"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.dci_id"/>
                                        </td>    
                                        <td>
                                            <span t-field="doc.dci_id.dosage"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.dci_id.forme"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.packaging_names"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.quantity"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.done"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.lot_names"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.expiration_dates"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.fabricant.name"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.all_inventory_diffs"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.total_qty_received"/>
                                         <!--   <span t-field="doc.available_qties"/> -->
                                        </td>
                                        <td>
                                            <span t-field="doc.all_quantities"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.name"/>
                                            <div style="border-bottom: 1px solid black; height: 20px; width: 100px;">
                                                <!-- This is an empty placeholder for manual input after printing -->
                                            </div>
                                        </td>

                                    </tr>
                                 </t> 
                               <!-- </tbody> -->

                              </table>

                           </div>
                            
                    </div> 
                </t>   
         </t>
      </t>
   </template>
