 <odoo>
 
 <template id="report_pvdereception">
      <t t-call="web.html_container">
         <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <div class="oe_structure"/>
                           <div> 
                           <h1 style="text-align:center;">Rapport de Réception des Produits</h1><br/>
                            <div class="row p-3 text-center">

                                <div class="col-3">
                                    <p><span >Region : </span> <span t-field="doc.region"/> </p>
                                    <p> <span> District de santé : </span>
                                    <span t-field="doc.district_sante"></span></p>
                                </div>

                                <div class="col-3">
                                  <p>
                                    <span>Aire de Santé : </span>
                                    <span t-field="doc.aire_sante"/> </p>
                                  
                                  <p>
                                    <span>Nom de la Structure : </span>
                                    <span t-field="doc.nom_de_la_structure" />  
                                  </p>


                                </div>

                                <div class="col-6">
                                    <p class="text-center"> <span>Fournisseur : </span>
                                    <span t-field="doc.partner_id"></span> </p>

                                    <div class="row">
                                        <div class="col-6">
                                           <p> <span> Date livraison :</span>
                                            <span t-field="doc.expiration_time"> Jours </span> </p>
                                            
                                            <p> N° BL <span t-field="doc.num_bl"/> </p>
                                          <!--    -->

                                        </div>
                                        <div class="col-6">
                                            <span>N° PVR: 
                                              <span t-field="doc.num_PVR"/>
                                            </span>

                                            <p>N°BC :  <span t-field="doc.num_bc"/> </p>
                                        
                                        </div>

                                        
                                    </div>
                               
                                    
                                </div>
                                
                            </div>
                              <table class="table">
                                <thead>
                                   <tr>
                                        <th name="th_numero" class="text-start">
                                            <span>N° d'ordre</span>
                                        </th>
                                        <th name="th_nom" class="text-start">
                                            <span>Nom (DCI) </span>
                                        </th>
                                        <th name="th_dosage" class="text-start">
                                            <span>Dosage</span>
                                        </th>
                                        <th name="th_forme" class="text-start">
                                            <span>Forme Galénique</span>
                                        </th>
                                        <th name="th_conditionement" class="text-start">
                                            <span>Conditionement</span>
                                        </th>
                                        <th name="th_quantité" class="text-start">
                                            <span>Quantité (Bon de Livraison)</span>
                                        </th>
                                        <th name="th_qtitéRe" class="text-start">
                                            <span>Quantité Reçue</span>
                                        </th>
                                        <th name="th_numérodelot" class="text-start">
                                            <span>Numéro de Lot</span>
                                        </th>
                                        <th name="th_date" class="text-start">
                                            <span>Date de Péremption</span>
                                        </th>
                                        <th name="th_fabricant" class="text-start">
                                            <span>Fabricant</span>
                                        </th>
                                         <th name="th_écart" class="text-start">
                                            <span>Ecart</span>
                                        </th>
                                        <th name="th_qtitéRej" class="text-start">
                                            <span>Quantité Rejetée</span>
                                        </th>
                                         <th name="th_qtitéAcc" class="text-start">
                                            <span>Quantité Acceptée</span>
                                        </th>
                                         <th name="th_observations" class="text-start">
                                            <span>Oberservation</span>
                                        </th>
                            
                                    </tr>
                                </thead>

                              <!--  <tbody>
                                 <t t-foreach="doc.product_fournisseur_ids" t-as="doc"> -->
                                  <tr>
                                        <td>
                                            <span t-field="doc.id"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.dci_id"/>
                                        </td>    
                                        <td>
                                            <span t-field="doc.dci_id.dosage"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.dci_id.forme"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.packaging_names"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.lot_id"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.name"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.lot_id"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.expiration_date"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.fabricant.name"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.inventory_diff"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.all_quantities"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.all_inventory_diffs"/>
                                        </td>
                                        <td>
                                            <span t-field="doc.detailed_type"/>
                                        </td>

                                    </tr>
                                <!-- </t> 
                                </tbody> -->

                              </table>

                           </div>
                            
                    </div> 
                </t>   
         </t>
      </t>
   </template>
